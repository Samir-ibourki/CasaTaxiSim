{
  "expo": {
    "name": "CasaTaxiSim",
    "slug": "CasaTaxiSim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "scheme": "casataxisim",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.samiribourki.casataxisim",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/icon.jpg",
        "backgroundImage": "./assets/icon.jpg",
        "monochromeImage": "./assets/icon.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBCB2E3yrXD4XqX6CJH_3lh0BZEzVYl5Mo"
        }
      }
    },

    "web": {
      "output": "static",
      "favicon": "./assets/icon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          //  "image": "./assets/images/splash-icon.png",
          // "imageWidth": 200,
          // "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-maps",
        {
          "requestLocationPermission": true,
          "locationPermission": "Allow $(PRODUCT_NAME) to use your location"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ab353bbf-b02e-4a3f-8b8f-74b806c78331"
      }
    }
  }
}
